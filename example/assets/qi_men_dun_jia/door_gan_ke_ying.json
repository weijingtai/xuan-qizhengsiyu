{
    "开": {
        "戊": "财名俱得。",
        "乙": "小财可求。路逢执杖老人，或哭泣，红衣公吏至，埋葬竖造主子孙富贵。",
        "丙": "贵人印绶。路逢携杖老人悲啼困苦，埋葬竖造子孙富贵。",
        "丁": "远信必至。路逢小儿持杖，戊癸日与天英会，飞鸟白鹤，雷鸣，主富贵。",
        "己": "事绪不定。",
        "庚": "道路词讼，谋为两岐。",
        "辛": "阴人道路。",
        "壬": "远行有失，注意破财。",
        "癸": "阴人失财小凶。"
    },
    "休": {
        "戊": "财物和合。",
        "乙": "求谋重，不得；求轻，可得。路逢牛马成群，或扛抬物具，埋葬竖造子孙繁衍，世代官禄。",
        "丙": "文书和合喜庆。五十里逢鼓乐声，有飞鸟风云白鹤应，埋葬竖造子孙富贵。",
        "丁": "百讼休歇。路逢皂衣人或女人，二十里应，埋葬竖造，主子孙富贵。",
        "己": "暗昧不宁，后吉。",
        "庚": "文书词讼先结后解。",
        "辛": "疾病迟愈，失物不得。",
        "壬":"阴人词讼牵连。生",
        "癸":"阴人词讼牵连。生"
    },
    "生": {
        "戊": "嫁娶、求财、谒贵皆吉。",
        "乙": "主阴人生产，迟吉。路逢两鼠相斗及孝服人，百鸟风云微雨车马求迎。破土安葬，主子孙繁衍，科甲恩荣。",
        "丙": "主贵人印绶、婚姻、书信喜事。路逢患眼病疾人，或相斗打，埋葬竖造子孙富贵。",
        "丁": "主词讼、婚姻、财利大吉。路逢鹰犬田猎，埋葬竖造主子孙繁盛，官禄不绝。",
        "己": "主得贵人维持，吉。",
        "庚": "主财产争讼破产，不利。",
        "辛": "主产妇疾病，后吉。",
        "壬": "主遗失财后得，贼盗易获。",
        "癸": "主婚姻不成，余事皆吉。"
    },
    "伤": {
        "戊": "主失脱难获。",
        "乙": "主求谋不得，反防盗失财。",
        "丙": "主道路损失。",
        "丁": "主音信不至。",
        "己": "主财散人病。",
        "庚": "主讼狱被刑杖，凶。",
        "辛": "主夫妻怀私恣怨。",
        "壬": "主因盗牵连。",
        "癸": "主讼狱被冤，有理难伸。"
    },
    "杜": {
        "戊": "主谋事不成，秘处求财得。",
        "乙": "宜暗求男人财物，后主不明致讼。",
        "丙": "主文契遗失。",
        "丁": "主男人讼狱。",
        "己": "主私谋害人招非。",
        "庚": "主因女人讼狱被刑。",
        "辛": "主打伤人，词讼，男人小口凶。",
        "壬": "主奸盗事，凶。",
        "癸": "主百事皆阻，病者不食。"
    },
    "景": {
        "戊": "因财产词讼，远行吉。",
        "乙": "主讼事不成。",
        "丙": "主文书急迫，火速不利。",
        "丁": "主因文书印状招非。",
        "己": "主官司牵连。",
        "庚": "主讼人自讼。",
        "辛": "主阴人词讼。",
        "壬": "主因贼牵连。",
        "癸": "主因奴婢受刑。"
    },
    "死": {
        "戊": "主作伪财。",
        "乙": "主求事不成。",
        "丙": "主信息忧疑。",
        "丁": "主老阳人疾病。",
        "己": "主病讼牵连不已，凶。",
        "庚": "主女人生产，母子俱凶。",
        "辛": "主盗贼失脱难获。",
        "壬": "主讼人自讼自招。",
        "癸": "主妇女嫁娶事凶。"
    },
    "惊": {
        "戊": "主损财，信阻。",
        "乙": "主谋财不得。",
        "丙": "主文书印信惊恐。",
        "丁": "主词讼牵连。",
        "己": "主恶犬伤人成讼。",
        "庚": "主道路损折，遇贼盗，凶。",
        "辛": "主女人成讼，凶。",
        "壬": "主官司囚禁，病者大凶。",
        "癸": "主被盗，失物难获。"
    }
